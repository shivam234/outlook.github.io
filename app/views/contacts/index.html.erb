<h1>My contacts</h1>
<table border="1">
  <tr>
    <th></th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
  </tr>
  <% @contacts.each do |contact| %>
    <% if !contact['GivenName'].blank? && !contact['Surname'].blank? %>
      <tr>
        <td>
          <input type="checkbox">/</input>
        </td>
        <td>
          <%= contact['GivenName'] rescue nil %>
        </td>
        <td>
          <%= contact['Surname'] rescue nil %>
        </td>
        <td>
          <%= contact['EmailAddresses'][0]['Address'] rescue nil %>
        </td>
      </tr>
    <% end %>
  <% end %>
</table>
